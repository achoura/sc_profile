{
  "name": "achoura/sc-profile",
  "description": "Social Commerce Profile",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8",
    "drush/drush": "~8.1.11",
    "drupal/drupal-extension": "~3.2.0",
    "behat/behat": "^3.0",
    "drupal/coder": "~8.2.0",
    "phing/phing": "^2.14",
    "drupal/devel": "^1.0",
    "squizlabs/php_codesniffer": "2.*",
    "se/selenium-server-standalone": "^2.53",
    "drupal/console": "^1.0",
    "jakoch/phantomjs-installer": "1.9.8",
    "composer/composer": "^1.4",
    "grasmash/yaml-cli": "^1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "malsup/cycle",
        "version": "dev-master",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/malsup/cycle/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "git@github.com:malsup/cycle.git",
          "type": "git"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "briancherne/jquery-hoverIntent",
        "version": "dev-master",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/briancherne/jquery-hoverIntent/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "git@github.com:briancherne/jquery-hoverIntent.git",
          "type": "git"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "woocommerce/FlexSlider",
        "version": "2.6.3",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/woocommerce/FlexSlider/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "git@github.com:woocommerce/FlexSlider.git",
          "type": "git"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "douglascrockford/JSON-js",
        "version": "dev-master",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/douglascrockford/JSON-js/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "git@github.com:douglascrockford/JSON-js.git",
          "type": "git"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mehrpadin/Superfish-for-Drupal",
        "version": "2.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/mehrpadin/Superfish-for-Drupal/archive/2.0.zip",
          "type": "zip"
        },
        "source": {
          "url": "git@github.com:mehrpadin/Superfish-for-Drupal.git",
          "type": "git"
        }
      }
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "extra": {
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "patches": {
      "drupal/commerce": {
        "Provide a Views default argument for the current user's cart ID": "https://www.drupal.org/files/issues/commerce-Views_argument_default_current_cart-2852363-5.patch"
      }
    }
  },
  "require": {
    "oomphinc/composer-installers-extender": "^1.1",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "cweagans/composer-patches": "^1.6.0",
    "acquia/lightning": "2.1.6",
    "drupal/bootstrap": "^3.5.0",
    "drupal/commerce": "2.0-beta6",
    "drupal/commerce_variation_add_to_cart": "1.x-dev",
    "angrycactus/commerce_cop": "dev-8.x-1.x",
    "drupal/admin_toolbar": "1.18.0",
    "drupal/flexslider": "2.0.0-beta1",
    "drupal/easy_breadcrumb": "1.6.0",
    "drupal/sitemap": "1.3.0",
    "drupal/devel": "1.0.0-rc2",
    "drupal/bg_image_formatter": "1.0-beta2",
    "drupal/default_content": "1.0.0-alpha4",
    "drupal/entityblock": "1.0.0-alpha4",
    "drupal/superfish": "1.0.0-rc6",
    "drupal/file_entity": "2.0.0-beta3",
    "drupal/views_slideshow": "4.4.0",
    "drupal/views_slideshow_cycle": "4.4.0",
    "drupal/social_media_links": "2.2.0",
    "drupal/menu_trail_by_path": "1.1.0",
    "drupal/menu_link_attributes": "1.0.0-beta3",
    "drupal/simple_sitemap": "2.9.0",
    "malsup/cycle": "dev-master",
    "woocommerce/FlexSlider": "2.6.3",
    "briancherne/jquery-hoverIntent": "dev-master",
    "douglascrockford/JSON-js":"dev-master",
    "mehrpadin/Superfish-for-Drupal":"2.0"
  }
}
